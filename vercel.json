  "builds": [
    { "src": "api/verify.js", "use": "@vercel/node" },
    { "src": "api/confirm.js", "use": "@vercel/node" },
    { "src": "api/form-submit.js", "use": "@vercel/node" }

  ],
  "routes": [
    { "src": "/api/verify", "dest": "/api/verify.js" },
